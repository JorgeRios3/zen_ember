<br><br>
<div class="h3 text text-info">Distribucion de Comisiones</div>
<div class="form-group">
	<label class="control-label">Etapa</label>
	<div class="row">
		<div class="col-md-5">
			{{#x-select value=selectedEtapa action=(action  (mut selectedEtapa)) class="form-control form-control-inline"}}
					{{#x-option value='0'}}Elija Etapa{{/x-option}}
					{{#each model as |item|}}
						{{#x-option value=item.id}}{{item.nombre}}{{/x-option}}
				{{/each}}
			{{/x-select}}
		</div>
	</div>
</div>
<div class="badge badge-info">{{listaComisiones.length}}</div>
<br>
<div id="no-more-tables">
		<table class="table table-bordered table-striped table-condensed cf">
		<thead>
		<tr>
			<th>Inmueble</th>
			<th>Manzana</th>
			<th>Lote</th>
			<th>Nombre Vendedor</th>
			<th>Nombre Gerente</th>
			<th>Porcentaje</th>
		</tr>
		</thead>
		<tbody>
		{{#each listaComisiones as |row|}}
			<tr>
				<td data-title="Inmueble"><span class="text-right">{{row.inmueble}}</span></td>
				<td data-title="Manzana">{{row.manzana}}</td>
				<td data-title="Lote">{{row.lote}}</td>
				<td data-title="Nombre Vendedor">{{row.nombrevendedor}}</td>
				<td data-title="Nombre Gerente">{{row.nombregerente}}</td>
				<td data-title="Porcentaje">{{row.porcentaje}}</td>
				
			</tr>
		{{/each}}
		</tbody>
		</table>
	</div>