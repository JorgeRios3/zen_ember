<div class="h3 text text-info">Inmuebles Arcadia</div>
<div class="form-group">
	<label for="etapas" class="control-label">Etapa</label>
	<div class="row">
		<div class="col-md-5">
			
			{{#x-select value='0' action="selectedEtapa" class="form-control form-control-inline"}}
	  			{{#x-option value='0'}}Elija etapa{{/x-option}}
	  			
	  			  {{#each etapas as |item|}}
	  				  {{#x-option value=item}}{{item.nombre}}{{/x-option}}
				  {{/each}}
				
			{{/x-select}}
		</div>
	</div>
</div>

{{#bsz-table titleCols=titleCols}}
	{{#each listaLotes as |record|}}
		{{bsz-row titleCols=titleCols record=record alignments=alignments}}
	{{/each}}
{{/bsz-table}}